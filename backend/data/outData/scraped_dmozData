        $(window).on(""resize"", function () {
            var negativeLeftMargin;
            currentWindowWidth = $(window).width();
            if (currentWindowWidth > 1170 && currentWindowWidth < 1921) {
                negativeLeftMargin = (1170 - currentWindowWidth) / 2 + ""px"";
                $("".heroCenter"").css({
                    ""margin-left"": negativeLeftMargin,
                    ""width"": currentWindowWidth
                });
            };
        });

        
    

        //change top margin as page is scrolled
        var topOffset = 0;
        $(window).scroll(function () {
            if ($(document).scrollTop() <= 40) {
                topOffset = 40 - $(document).scrollTop();
                $('.siteLogo').css({ ""margin-top"": topOffset + ""px"" });
                $('.navbar.navbar-inverse').css({ ""margin-top"": topOffset + 10 + ""px"" });
            } else {
                $('.siteLogo').css({ ""margin-top"": ""10px"" });
                $('.navbar.navbar-inverse').css({ ""margin-top"": ""10px"" });
            }
        });
    
