        $(function () {
            var maxcount = 6;
            var imagesToShow;
            igfeedurl = ""https://api.instagram.com/v1/users/self/media/recent/?access_token=2122540818.b86753a.4ab22fd16c87471090c1c7a9e91fc053"";

            $.getJSON(igfeedurl, function (data) {
                if (data.data.length > maxcount) {
                    imagesToShow = maxcount;
                }
                else {
                    imagesToShow = data.data.length;
                }
                for (var i = 0; i < imagesToShow; i++) {
                    $("".instagramFeed"").append(""<a title='"" + data.data[i].likes.count + "" likes and "" + data.data[i].comments.count + "" comments' href='"" + data.data[i].link + ""' target='_blank'><img src='"" + data.data[i].images.standard_resolution.url + ""'/></a>"");
                }
            });
        });
    








